{
  "name": "udemy-translate-subtitles",
  "version": "1.0.0",
  "description": "Subtitle translation for Udemy",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "start": "yarn webpack:dev",
    "build": "concurrently --kill-others-on-fail \"npm run typecheck\" \"npm run lint\" \"npm run format\" \"npm run webpack:prod\"",
    "webpack:dev": "webpack --watch --progress --config webpack.dev.js",
    "webpack:prod": "webpack --config webpack.prod.js",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,sass,json}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint",
    "src/**/*.{js,jsx,ts,tsx,css,scss,sass,json}": "prettier --write"
  },
  "author": "dev.fachory",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "^0.0.178",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.6.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier": "2.5.1",
    "sass": "^1.49.7",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "webpack": "^5.69.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
